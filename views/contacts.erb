<h2>All Contacts</h2>



<li><a href="/contacts/new">Add a new contact</a></li>
<li><a href="/contacts/:id/edit">Edit contact information</a></li>


<ul>
  <% @contacts.each do |contact| %>
    <li><a href="/contacts/<%= contact.id %>"><%= contact.first_name %> <%= contact.last_name %></a></li>
  <% end %>
</ul>